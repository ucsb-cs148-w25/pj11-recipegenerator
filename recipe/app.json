{
  "expo": {
    "name": "RecipeAI",
    "slug": "RecipeAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/app_icon/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "RecipeAI",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "plugins": ["@react-native-google-signin/google-signin"],
    "ios": {
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.jasoninthebox.RecipeAI",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "RecipeAI",
              "com.googleusercontent.apps.1075996537970-k52kpdt259g53acl1k31jf4f22uld8ep"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.jasoninthebox.RecipeAI",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon/android/res/mipmap-mdpi/ic_launcher_foreground.png",
        "backgroundImage": "./assets/app_icon/android/res/mipmap-hdpi/ic_launcher_background.png"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{ "scheme": "RecipeAI" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/app_icon/web/favicon.png",
      "manifest": {
        "short_name": "RecipeAI",
        "name": "RecipeAI",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./assets/app_icon/web/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/app_icon/web/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "owner": "jasoninthebox",
    "extra": {
      "eas": {
        "projectId": "8048451a-a19d-48b2-9192-c2402ac2b276"
      }
    }
  }
}
