{
  "expo": {
    "name": "StickerSmash",
    "slug": "StickerSmash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./recipe/assets/app_icon/icon.png",
    "scheme": "https://auth.expo.io/@expoid/StickerSmash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./recipe/assets/app_icon/ios/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./recipe/assets/app_icon/android/foreground.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./recipe/assets/app_icon/android/icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./recipe/assets/app_icon/web/favicon.ico",
      "manifest": {
        "short_name": "RecipeGenerator",
        "name": "Recipe Generator",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./recipe/assets/app_icon/web/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./recipe/assets/app_icon/web/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./recipe/assets/app_icon/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "google": {
        "iosClientId": "http://localhost:8000",
        "androidClientId": "http://localhost:8000"
      }
    }
  }
}