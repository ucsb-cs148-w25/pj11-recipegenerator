{
  "expo": {
    "name": "StickerSmash",
    "slug": "stickersmash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "stickersmash",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "ios": {
      "icon": "./assets/icon.png",
      "bundleIdentifier": "com.jasoninthebox.StickerSmash",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "stickersmash"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon/android/res/mipmap-mdpi/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jasoninthebox.StickerSmash",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "stickersmash"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/app_icon/web/favicon.ico",
      "manifest": {
        "short_name": "StickerSmash",
        "name": "StickerSmash",
        "start_url": ".",
        "display": "standalone",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "icons": [
          {
            "src": "./assets/app_icon/web/icon-192.png",
            "sizes": "192x192",
            "type": "image/png"
          },
          {
            "src": "./assets/app_icon/web/icon-512.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      }
    },
    "plugins": [

    ],
    "extra": {
      "eas": {
        "projectId": "8048451a-a19d-48b2-9192-c2402ac2b276"
      }
    }
  }
}
